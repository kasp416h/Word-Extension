{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "word",
  "title": "Word",
  "description": "Commands to optimize word production",
  "icon": "word.png",
  "author": "kasp416h",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "newDocument",
      "title": "Create New Document",
      "description": "Opens a new word document",
      "mode": "no-view"
    },
    {
      "name": "recentDocuments",
      "title": "Show Recent Documents",
      "description": "Shows the last modified documents",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "eslint": "^9.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
